{
  "expo": {
    "name": "Radio Folklor",
    "description": "Radio de la tradition maroccaine",
    "scheme": "spotifyclone",
    "slug": "spotifyclone",
    "privacy": "public",
    "version": "0.5.0",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "githubUrl": "https://github.com/calebnance/expo-spotify",
    "orientation": "portrait",
    "primaryColor": "#181414",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#181414",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "versionCode": 50,
      "package": "com.radiofolklor"
    },
    "ios": {
      "buildNumber": "0.5.0",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "extra": {
      "eas": {
        "projectId": "cfe3186a-3491-4cad-8a37-4dad578b9b90"
      }
    }
  }
}
